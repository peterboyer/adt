{
	"name": "unenum",
	"version": "1.0.0",
	"license": "MIT",
	"author": "Peter Boyer <https://github.com/peterboyer>",
	"repository": "github:peterboyer/unenum",
	"description": "0kb, Rust-like Enums for TypeScript.",
	"scripts": {
		"test": "./scripts/test.sh",
		"test:e2e": "./scripts/test-e2e.sh",
		"lint": "./scripts/lint.sh",
		"lint:ts": "./scripts/lint-ts.sh",
		"build": "./scripts/build.sh"
	},
	"main": "./cjs/index.js",
	"module": "./mjs/index.js",
	"types": "./index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./mjs/index.js",
			"default": "./cjs/index.js"
		},
		"./global": {
			"types": "./global.d.ts",
			"import": "./mjs/global.js",
			"default": "./cjs/global.js"
		},
		"./global.enum": {
			"types": "./global.enum.d.ts",
			"import": "./mjs/global.js",
			"default": "./cjs/global.js"
		},
		"./global.result": {
			"types": "./global.result.d.ts",
			"import": "./mjs/global.js",
			"default": "./cjs/global.js"
		},
		"./global.future": {
			"types": "./global.future.d.ts",
			"import": "./mjs/global.js",
			"default": "./cjs/global.js"
		}
	},
	"devDependencies": {
		"@tsconfig/esm": "^1.0.2",
		"@tsconfig/strictest": "^2.0.0",
		"@types/jest": "^29.4.1",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"concurrently": "^8.0.1",
		"dot-json": "^1.2.2",
		"esbuild": "^0.17.15",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"prettier": "^2.8.4",
		"tsd": "^0.28.1",
		"typescript": "^5.0.3"
	},
	"keywords": [
		"es",
		"enum",
		"result",
		"future",
		"match",
		"ok",
		"error",
		"async",
		"promise",
		"monad",
		"fp",
		"functional",
		"ts",
		"typescript"
	]
}
