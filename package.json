{
	"name": "unenum",
	"version": "2.0.0",
	"license": "MIT",
	"author": "Peter Boyer <https://github.com/peterboyer>",
	"repository": "github:peterboyer/unenum",
	"description": "Universal ADT utilities for TypeScript.",
	"scripts": {
		"test": "./scripts/test.sh",
		"test:e2e": "./scripts/test-e2e.sh",
		"lint": "./scripts/lint.sh",
		"lint:ts": "./scripts/lint-ts.sh",
		"build": "./scripts/build.sh",
		"all": "yarn lint && yarn lint:ts && yarn test && yarn build && yarn test:e2e",
		"docs": "concurrently \"npx markserv README.md\" \"yarn docs:watch\"",
		"docs:build": "./scripts/docs-build.sh",
		"docs:watch": "chokidar README.ts -c \"yarn docs:build\""
	},
	"type": "module",
	"main": "./index.js",
	"types": "./index.d.ts",
	"exports": "./index.js",
	"devDependencies": {
		"@tsconfig/esm": "^1.0.2",
		"@tsconfig/strictest": "^2.0.0",
		"@types/jest": "^29.4.1",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^8.0.1",
		"dot-json": "^1.2.2",
		"esbuild": "^0.17.15",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"prettier": "^2.8.4",
		"tsd": "^0.28.1",
		"typescript": "^5.0.3"
	},
	"keywords": [
		"ts",
		"typescript",
		"adt",
		"enum",
		"union",
		"variant",
		"result",
		"future",
		"match",
		"ok",
		"error",
		"fp",
		"sum",
		"box",
		"boxed",
		"monad"
	]
}
